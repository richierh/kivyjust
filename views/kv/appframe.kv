<Screen2@Screen>:
    id : screen2
    name:"screen2"

    ScreenManager:
        id:switchscreen
        Screen:
            id:screenshow
            name:"Screen 1"
            BoxLayout:
                orientation: 'vertical'

                MDToolbar:
                    title: "Navigation Drawer"
                    elevation: 10
                    left_action_items: [['menu', lambda x: nav_drawer.toggle_nav_drawer()]]

                Button:
                    id:ActivateButtonCamera
                    text:"Scan Camera"
                    on_release:root.camera_on()
            
        Screen:
            id:screenshow2
            name:"Tampilkan Camera"
            BoxLayout:
                orientation: 'vertical'
                ZBarCam:
                    id: zbarcam
                    # optional, by default checks all types
                    # sistem barcode
                    code_types: 'QRCODE', 'EAN13'
                Label:
                    size_hint: None, None
                    size: self.texture_size[0], 50
                    text: ', '.join([str(symbol.data) for symbol in zbarcam.symbols])

    MDNavigationDrawer:
        id: nav_drawer

        ContentNavigationDrawer:


